{
  "name": "vaened/laroute",
  "description": "An enhanced Laravel extension to export named routes to JavaScript, allowing you to use route names instead of URLs. Supports route grouping for modular applications.",
  "keywords": [
    "laravel-library",
    "routes",
    "routing"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Vaened\\Laroute\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Enea Dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.21|^v11.15",
    "vaened/support": "^4.0",
    "lambdish/phunctional": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "orchestra/testbench": "^8|^9",
    "mockery/mockery": "^1.6"
  }
}
